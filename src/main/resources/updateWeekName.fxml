<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefWidth="200">
      <children>
         <VBox prefHeight="156.0" prefWidth="291.0" spacing="10.0" style="-fx-background-color: #D7DAE5;">
            <children>
               <Label fx:id="windowLabel" text="Update Week Name">
                  <font>
                     <Font name="SWItal" size="18.0" />
                  </font>
               </Label>
               <Label fx:id="nameLabel" text="Week Name">
                  <padding>
                     <Insets bottom="-10.0" />
                  </padding>
               </Label>
               <TextField fx:id="name" promptText="Week Name (Required)">
                  <font>
                     <Font name="Verdana" size="12.0" />
                  </font>
               </TextField>
               <Button fx:id="saveButton" mnemonicParsing="false" text="Save Name">
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </Button>
            </children>
            <padding>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
         </VBox>
      </children></AnchorPane>
</Scene>
